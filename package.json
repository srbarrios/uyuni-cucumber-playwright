{
  "author": "Oscar Barrios",
  "license": "MIT",
  "name": "playwright-cucumber-skeleton",
  "version": "1.0.0",
  "description": "This project's goal is to establish a baseline project for using Playwright with Cucumber.",
  "main": "index.js",
  "scripts": {
    "cucumber": "NODE_OPTIONS=\"--disable-warning=DEP0180\" npx cucumber-js --loader ts-node/esm --config config/cucumber.cjs",
    "cucumber:core": "npm run cucumber -- --profile core",
    "cucumber:reposync": "npm run cucumber -- --profile reposync",
    "cucumber:init_clients": "npm run cucumber -- --profile init_clients",
    "cucumber:proxy": "npm run cucumber -- --profile proxy"
  },
  "type": "module",
  "keywords": [],
  "devDependencies": {
    "@cucumber/cucumber": "^12.2.0",
    "@playwright/test": "^1.55.1",
    "@types/js-yaml": "^4.0.9",
    "@types/ssh2": "^1.15.5",
    "@types/tmp": "^0.2.6",
    "@types/xml2js": "^0.4.14",
    "@types/xmlrpc": "^1.3.10",
    "js-yaml": "^4.1.0",
    "ts-node": "^10.9.2",
    "xml2js": "^0.6.2"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "redis": "^5.8.2",
    "ssh2": "^1.17.0",
    "tmp": "^0.2.5",
    "xmlrpc": "^1.3.2"
  }
}
